{
  "name": "kidoicons",
  "version": "1.0.2",
  "description": "kidoicons is pixel perfect open source font icon pack crafted specially for those who wish to make a stunning designs",
  "main": "index.js",
  "scripts": {
    "minify:css": "npx postcss src/css/kidoicons.css > dist/css/kidoicons.min.css",
    "build:ts": "tsc",
    "build:rollup": "rollup -c",
    "build": "npm run minify:css && npm run build:ts && npm run build:rollup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devbeldotcom/kidoicons.git"
  },
  "keywords": [
    "kidoicons",
    "kidowebicons",
    "fonticons",
    "kidofonticons",
    "svgicons",
    "kidosvgicons",
    "svg"
  ],
  "author": "devbel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devbeldotcom/kidoicons/issues"
  },
  "homepage": "https://github.com/devbeldotcom/kidoicons#readme",
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@types/css-modules": "^1.0.5",
    "cssnano": "^7.0.6",
    "eslint": "^9.19.0",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.8.1"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "rollup": "^4.34.1",
    "typescript": "^5.7.3"
  }
}
